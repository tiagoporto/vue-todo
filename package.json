{
  "name": "vue-todo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test:unit",
    "cover": "vue-cli-service test:unit --coverage",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls",
    "lint": "vue-cli-service lint",
    "check-types": "tsc --noEmit",
    "check-formatting": "prettier --check '{src,public}/**/*.{js,ts,vue,json}'",
    "deploy": "npm run build && gh-pages -d dist",
    "checks": "npm run check-formatting && npm run lint && npm run check-types && npm run coveralls"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "core-js": "^3.4.4",
    "luxon": "^1.21.3",
    "normalize.css": "^8.0.1",
    "register-service-worker": "^1.6.2",
    "uuid": "^3.3.3",
    "vue": "^2.6.10",
    "vue-datetime": "^1.0.0-beta.11",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2",
    "weekstart": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/luxon": "^1.21.0",
    "@types/uuid": "^3.4.6",
    "@types/vue-datetime": "^1.0.0",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-pwa": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-typescript": "^4.1.0",
    "@vue/cli-plugin-unit-jest": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "coveralls": "^3.0.9",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-vue": "^5.0.0",
    "gh-pages": "^2.2.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "style-resources-loader": "^1.3.3",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "typescript": "~3.5.3",
    "vue-template-compiler": "^2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
