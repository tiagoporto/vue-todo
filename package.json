{
  "name": "vue-todo",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "checks": "npm run format:check && npm run lint && npm run typecheck",
    "lint": "vue-cli-service lint",
    "typecheck": "tsc --noEmit",
    "format:check": "prettier --check '{src,public}/**/*.{js,ts,vue,json}'",
    "format": "npm run format:check -- --write",
    "test": "vue-cli-service test:unit",
    "coverage": "vue-cli-service test:unit --coverage",
    "test:mutation": "stryker run --reporters html",
    "test:mutation-report": "stryker run --reporters dashboard"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "core-js": "^3.4.4",
    "luxon": "^1.21.3",
    "normalize.css": "^8.0.1",
    "register-service-worker": "^1.6.2",
    "uuid": "^3.3.3",
    "vue": "^2.6.10",
    "vue-datetime": "^1.0.0-beta.11",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2",
    "weekstart": "^1.0.1"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^2.4.0",
    "@stryker-mutator/html-reporter": "^2.4.0",
    "@stryker-mutator/jest-runner": "^2.4.0",
    "@stryker-mutator/typescript": "^2.4.0",
    "@types/jest": "^24.0.19",
    "@types/luxon": "^1.21.0",
    "@types/uuid": "^3.4.6",
    "@types/vue-datetime": "^1.0.0",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-pwa": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-typescript": "^4.1.0",
    "@vue/cli-plugin-unit-jest": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^4.0.4",
    "prettier": "^1.19.1",
    "style-resources-loader": "^1.3.3",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "typescript": "~3.5.3",
    "vue-template-compiler": "^2.6.10"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run format:check && npm run lint && npm run typecheck"
    }
  }
}
